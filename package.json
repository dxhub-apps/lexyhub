    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "playwright": "^1.56.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "^3.3.0",
    "stripe": "14.25.0",
    "tailwind-merge": "^3.3.1",
    "zod": "3.23.8"
