    "eslint-config-next": "14.2.5",
    "husky": "9.1.2",
    "lint-staged": "15.2.7",
    "playwright-core": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.16.2",
    "typescript": "5.4.5",
    "vitest": "^1.6.1"
